{
  "name": "@braks/revue-flow",
  "version": "0.0.37",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/bcakmakoglu/revue-flow"
  },
  "license": "MIT",
  "author": "Burak Cakmakoglu",
  "exports": {
    ".": {
      "import": "./dist/revue-flow.esm.js",
